{
  "name": "@shopgate/theme-gmd",
  "version": "4.5.0",
  "description": "Shopgate's Google Material app theme.",
  "main": "index.jsx",
  "repository": "shopgate/theme-gmd",
  "bugs": "https://github.com/shopgate/theme-gmd/issues",
  "homepage": "https://github.com/shopgate/theme-gmd",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint --parser babel-eslint --ignore-path .gitignore --ignore-path .eslintignore  --ext .js --ext .jsx .",
    "precommit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "version": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && bump extension-config.json -v $PACKAGE_VERSION -y && git add -A",
    "postversion": "git push && git push --tags"
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "*.js": "eslint --parser babel-eslint --ignore-path .gitignore --ignore-path .eslintignore --ext .js --ext .jsx ."
    }
  },
  "pre-commit": [
    "precommit"
  ],
  "dependencies": {
    "@shopgate/pwa-common": "^2.5.0",
    "@shopgate/pwa-common-commerce": "^3.1.1",
    "@shopgate/pwa-core": "^1.4.0",
    "@shopgate/pwa-tracking": "^1.2.9",
    "@shopgate/pwa-webcheckout-shopify": "^1.1.10",
    "@shopgate/react-hammerjs": "^0.5.3",
    "@shopgate/tracking-core": "^1.0.12",
    "babel-core": "^6.26.0",
    "babel-plugin-module-resolver": "3.0.0-beta.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "color": "^2.0.0",
    "css-spring": "git+https://github.com/shopgate/css-spring.git#master",
    "glamor": "^2.20.40",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hammerjs": "git+https://github.com/JedWatson/react-hammerjs.git#master",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "3.0.0-beta.6",
    "react-portal": "^3.1.0",
    "react-redux": "^5.0.5",
    "react-sizeme": "^2.3.6",
    "react-swipeable": "^4.0.1",
    "react-tap-event-plugin": "^3.0.2",
    "react-templates": "^0.6.1",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@shopgate/eslint-config": "^1.0.4",
    "@shopgate/pwa-unit-test": "^1.3.0",
    "coveralls": "^3.0.0",
    "eslint-import-resolver-babel-module": "4.0.0-beta.1",
    "eslint-plugin-import": "^2.7.0",
    "lint-staged": "^4.2.3",
    "react-addons-test-utils": "^15.6.2",
    "react-transform-catch-errors": "^1.0.2",
